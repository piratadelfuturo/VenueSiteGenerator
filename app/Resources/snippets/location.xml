<?xml version="1.0" ?>

<template xmlns="http://schemas.sulu.io/template/template"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://schemas.sulu.io/template/template http://schemas.sulu.io/template/template-1.0.xsd">

    <key>location</key>

    <meta>
        <title lang="fr">Location</title>
        <title lang="en">Location</title>
    </meta>

    <properties>
        <property name="title" type="text_line" mandatory="true">
            <meta>
                <title lang="fr">Titel</title>
                <title lang="en">Title</title>
            </meta>

            <tag name="sulu.node.name"/>
        </property>
        <section name="location_data">
        <property name="location" type="location">
            <meta>
                <title lang="fr">Location</title>
                <title lang="en">Location</title>
            </meta>

        </property>
        
        <block name="phones" default-type="phone" minOccurs="0">
            <types>
                <type name="phone">
                    <properties>
                        <property name="phone" type="phone"></property>
                    </properties>
                </type>
            </types>
        </block>
        <block name="faxes" default-type="fax" minOccurs="0">
            <types>
                <type name="fax">
                    <properties>
                        <property name="phone" type="phone"></property>
                    </properties>
                </type>

            </types>            
        </block>
        <block name="emails" default-type="email" minOccurs="0">
            <types>
                <type name="email">
                    <properties>
                        <property name="email" type="email"></property>
                    </properties>
                </type>
            </types>
        </block>
        </section>
    </properties>
    </properties>
</template>
