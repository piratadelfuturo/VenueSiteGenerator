<?xml version="1.0" ?>

<template xmlns="http://schemas.sulu.io/template/template"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://schemas.sulu.io/template/template http://schemas.sulu.io/template/template-1.0.xsd">

    <key>location</key>

    <meta>
        <title lang="fr">Location</title>
        <title lang="en">Location</title>
    </meta>

    <properties>
        <property name="title" type="text_line" mandatory="true">
            <meta>
                <title lang="fr">Titel</title>
                <title lang="en">Title</title>
            </meta>

            <tag name="sulu.node.name"/>
        </property>
        <property name="location" type="location">
            <meta>
                <title lang="fr">Location</title>
                <title lang="en">Location</title>
            </meta>
        </property>
        <property name="phone" type="phone">
            <meta>
                <title lang="fr">Phone number</title>
                <title lang="en">Phone number</title>
            </meta>
        </property>
        <property name="fax" type="phone">
            <meta>
                <title lang="fr">Fax</title>
                <title lang="en">Fax</title>
            </meta>
        </property>
        <property name="email" type="email">
            <meta>
                <title lang="fr">E-Mail</title>
                <title lang="en">E-Mail</title>
            </meta>
        </property>
        
    </properties>
</template>
